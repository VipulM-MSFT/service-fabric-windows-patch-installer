<?xml version="1.0" encoding="utf-8"?>
<ApplicationManifest xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" ApplicationTypeName="PatchInstallerAppType" ApplicationTypeVersion="1.0.0" xmlns="http://schemas.microsoft.com/2011/01/fabric">
  <Parameters>
    <Parameter Name="PatchInstallerService_InstanceCount" DefaultValue="-1" />
  </Parameters>
  <ServiceManifestImport>
    <ServiceManifestRef ServiceManifestName="PatchInstallerServicePkg" ServiceManifestVersion="1.0.0" />
    <Policies>
      <RunAsPolicy CodePackageRef="Code" UserRef="PatchInstallerUser" />
    </Policies>
  </ServiceManifestImport>
  <DefaultServices>
    <Service Name="PatchInstallerService">
      <StatelessService ServiceTypeName="PatchInstallerServiceType" InstanceCount="[PatchInstallerService_InstanceCount]">
        <SingletonPartition />
      </StatelessService>
    </Service>
  </DefaultServices>
  <Principals>
    <Users>
      <User Name="PatchInstallerUser" AccountType="LocalSystem" />
    </Users>
  </Principals>
</ApplicationManifest>